    "version",
    "washingtondc"
  ],
  "license": "MIT",
  "lint-staged": {
    "**/!(logo).(jpg|png)": [
      "task image:compress --"
    ],
    "**/*": [
      "task lint:any --"
    ],
    "**/*.(css|html|less|md|mjml|php|scss|sh|xml)": [
      "task fix:prettier --"
    ],
    "**/*.(js|jsx|ts|tsx)": [
      "task fix:js --",
      "task lint:js --"
    ],
    "**/*.json": [
      "task fix:json --"
    ],
    "**/*.sh": [
      "task lint:shell --"
    ],
    "**/*.toml": [
      "task lint:toml --"
    ],
    "**/*.xml": [
      "task fix:xml --",
      "task lint:xml --"
    ],
    "**/*.yml": [
      "task fix:yaml --",
      "task lint:yaml --"
    ],
    "logo.png": [
      "task image:resize -- 200 200",
      "task image:compress --"
    ]
  },
  "main": "index.js",
  "name": "semantic-release-github",
  "nodemonConfig": {
    "exec": "tsc -p . && node --inspect-brk dist/main",
    "ext": "js,jsx,json,ts,tsx,yml",
    "ignore": [
      "src/**/*.spec.ts"
    ],
    "watch": [
      ".env",
      "src"
    ]
  },
  "ntl": {
    "descriptions": {
      "help": "Display a list of tasks that can be run via the Taskfile.yml",
      "prepare": "Hook, triggered by running npm install, that runs logic such as installing pre-commit hooks",
      "repair": "Fixes basic issues in projects that are having trouble running basic commands like 'task start'",
      "start": "Entrypoint for new developers that installs requirements and then runs 'task start'"
    }
  },
  "nyc": {
    "all": true,
    "include": [
      "lib/**/*.js",
      "index.js"
    ],
    "reporter": [
      "json",
      "text",
      "html"
    ]
  },
  "optionalDependencies": {},
  "peerDependencies": {
    "semantic-release": ">=18.0.0-beta.1"
  },
  "prettier": "@washingtondc/prettier",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main",
      "master",
      "next",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "extends": "semantic-release-config"
  },
  "renovate": {
    "extends": [
      "github>semantic-release/.github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ProfessorManhattan/semantic-release-github.git"
  },
  "scripts": {
    "codecov": "codecov -f coverage/coverage-final.json",
    "help": "task --list",
    "lint": "xo",
    "prepare": "task common:husky",
    "pretest": "npm run lint",
    "repair": "bash <(curl -sS https://install.doctor/repair)",
    "semantic-release": "semantic-release",
    "start": "bash .config/start.sh",
    "test": "nyc ava -v",
    "test:ci": "nyc ava -v"
  },
  "type": "commonjs",
  "typings": "dist/main.d.ts",
  "version": "0.0.1",
  "xo": {
    "prettier": true,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "unicorn/no-reduce": "off",
      "unicorn/string-content": "off"
    },
    "space": true
  }
}
