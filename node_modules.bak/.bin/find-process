#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hawkwood/semantic-release-github/node_modules/.pnpm/find-process@1.4.7/node_modules/find-process/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/find-process@1.4.7/node_modules/find-process/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/find-process@1.4.7/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/node_modules:/home/hawkwood/semantic-release-github/node_modules:/home/hawkwood/node_modules:/home/node_modules:/node_modules:/home/hawkwood/semantic-release-github/node_modules/find-process/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/find-process/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/hawkwood/semantic-release-github/node_modules/.pnpm/find-process@1.4.7/node_modules/find-process/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/find-process@1.4.7/node_modules/find-process/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/find-process@1.4.7/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/node_modules:/home/hawkwood/semantic-release-github/node_modules:/home/hawkwood/node_modules:/home/node_modules:/node_modules:/home/hawkwood/semantic-release-github/node_modules/find-process/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/find-process/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../find-process/bin/find-process.js" "$@"
else
  exec node  "$basedir/../find-process/bin/find-process.js" "$@"
fi
