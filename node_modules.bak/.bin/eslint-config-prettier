#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hawkwood/semantic-release-github/node_modules/.pnpm/eslint-config-prettier@8.4.0_eslint@8.9.0/node_modules/eslint-config-prettier/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/eslint-config-prettier@8.4.0_eslint@8.9.0/node_modules/eslint-config-prettier/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/eslint-config-prettier@8.4.0_eslint@8.9.0/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/node_modules:/home/hawkwood/semantic-release-github/node_modules:/home/hawkwood/node_modules:/home/node_modules:/node_modules:/home/hawkwood/semantic-release-github/node_modules/eslint-config-prettier/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/eslint-config-prettier/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/hawkwood/semantic-release-github/node_modules/.pnpm/eslint-config-prettier@8.4.0_eslint@8.9.0/node_modules/eslint-config-prettier/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/eslint-config-prettier@8.4.0_eslint@8.9.0/node_modules/eslint-config-prettier/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/eslint-config-prettier@8.4.0_eslint@8.9.0/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/node_modules:/home/hawkwood/semantic-release-github/node_modules:/home/hawkwood/node_modules:/home/node_modules:/node_modules:/home/hawkwood/semantic-release-github/node_modules/eslint-config-prettier/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/eslint-config-prettier/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../eslint-config-prettier/bin/cli.js" "$@"
else
  exec node  "$basedir/../eslint-config-prettier/bin/cli.js" "$@"
fi
