#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hawkwood/semantic-release-github/node_modules/.pnpm/rxjs-report-usage@1.0.6/node_modules/rxjs-report-usage/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/rxjs-report-usage@1.0.6/node_modules/rxjs-report-usage/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/rxjs-report-usage@1.0.6/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/node_modules:/home/hawkwood/semantic-release-github/node_modules:/home/hawkwood/node_modules:/home/node_modules:/node_modules:/home/hawkwood/semantic-release-github/node_modules/rxjs-report-usage/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/rxjs-report-usage/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/hawkwood/semantic-release-github/node_modules/.pnpm/rxjs-report-usage@1.0.6/node_modules/rxjs-report-usage/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/rxjs-report-usage@1.0.6/node_modules/rxjs-report-usage/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/rxjs-report-usage@1.0.6/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/node_modules:/home/hawkwood/semantic-release-github/node_modules:/home/hawkwood/node_modules:/home/node_modules:/node_modules:/home/hawkwood/semantic-release-github/node_modules/rxjs-report-usage/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/rxjs-report-usage/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rxjs-report-usage/bin/rxjs-report-usage" "$@"
else
  exec node  "$basedir/../rxjs-report-usage/bin/rxjs-report-usage" "$@"
fi
