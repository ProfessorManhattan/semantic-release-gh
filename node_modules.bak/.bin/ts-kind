#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hawkwood/semantic-release-github/node_modules/.pnpm/tsutils-etc@1.4.1_tsutils@3.21.0+typescript@4.5.5/node_modules/tsutils-etc/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/tsutils-etc@1.4.1_tsutils@3.21.0+typescript@4.5.5/node_modules/tsutils-etc/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/tsutils-etc@1.4.1_tsutils@3.21.0+typescript@4.5.5/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/node_modules:/home/hawkwood/semantic-release-github/node_modules:/home/hawkwood/node_modules:/home/node_modules:/node_modules:/home/hawkwood/semantic-release-github/node_modules/tsutils-etc/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/tsutils-etc/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/hawkwood/semantic-release-github/node_modules/.pnpm/tsutils-etc@1.4.1_tsutils@3.21.0+typescript@4.5.5/node_modules/tsutils-etc/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/tsutils-etc@1.4.1_tsutils@3.21.0+typescript@4.5.5/node_modules/tsutils-etc/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/tsutils-etc@1.4.1_tsutils@3.21.0+typescript@4.5.5/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/node_modules:/home/hawkwood/semantic-release-github/node_modules:/home/hawkwood/node_modules:/home/node_modules:/node_modules:/home/hawkwood/semantic-release-github/node_modules/tsutils-etc/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/tsutils-etc/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsutils-etc/bin/ts-kind" "$@"
else
  exec node  "$basedir/../tsutils-etc/bin/ts-kind" "$@"
fi
