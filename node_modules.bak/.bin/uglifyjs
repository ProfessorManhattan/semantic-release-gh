#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hawkwood/semantic-release-github/node_modules/.pnpm/uglify-js@3.15.3/node_modules/uglify-js/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/uglify-js@3.15.3/node_modules/uglify-js/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/uglify-js@3.15.3/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/node_modules:/home/hawkwood/semantic-release-github/node_modules:/home/hawkwood/node_modules:/home/node_modules:/node_modules:/home/hawkwood/semantic-release-github/node_modules/uglify-js/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/uglify-js/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/hawkwood/semantic-release-github/node_modules/.pnpm/uglify-js@3.15.3/node_modules/uglify-js/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/uglify-js@3.15.3/node_modules/uglify-js/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/uglify-js@3.15.3/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/node_modules:/home/hawkwood/semantic-release-github/node_modules:/home/hawkwood/node_modules:/home/node_modules:/node_modules:/home/hawkwood/semantic-release-github/node_modules/uglify-js/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/uglify-js/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../uglify-js/bin/uglifyjs" "$@"
else
  exec node  "$basedir/../uglify-js/bin/uglifyjs" "$@"
fi
