#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hawkwood/semantic-release-github/node_modules/.pnpm/npm@8.5.1/node_modules/npm/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/npm@8.5.1/node_modules/npm/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/npm@8.5.1/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/node_modules:/home/hawkwood/semantic-release-github/node_modules:/home/hawkwood/node_modules:/home/node_modules:/node_modules:/home/hawkwood/semantic-release-github/node_modules/npm/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/npm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/hawkwood/semantic-release-github/node_modules/.pnpm/npm@8.5.1/node_modules/npm/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/npm@8.5.1/node_modules/npm/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/npm@8.5.1/node_modules:/home/hawkwood/semantic-release-github/node_modules/.pnpm/node_modules:/home/hawkwood/semantic-release-github/node_modules:/home/hawkwood/node_modules:/home/node_modules:/node_modules:/home/hawkwood/semantic-release-github/node_modules/npm/bin/node_modules:/home/hawkwood/semantic-release-github/node_modules/npm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../npm/bin/npx-cli.js" "$@"
else
  exec node  "$basedir/../npm/bin/npx-cli.js" "$@"
fi
